{
  "version": "1.0",
  "description": "Conductor Framework Configuration",
  
  "debug": {
    "enabled": true,
    "max_tensor_elements": 100,
    "indent_size": 2
  },
  
  "cache": {
    "enabled": false,
    "max_size_mb": 1024,
    "cache_dir": null
  },
  
  "compilation": {
    "timeout_seconds": 300,
    "optimization_level": "O2",
    "enable_fusion": true
  },
  
  "runtime": {
    "device": "gcu",
    "memory_pool_size_mb": 512,
    "enable_profiling": false
  },
  
  "logging": {
    "level": "DEBUG",
    "enable_file_logging": true,
    "log_file": "conductor.log"
  }
}
